# .inputrc
"\ee": shell-expand-line
"\eh": backward-kill-word
"\C-xp": history-search-backward
"\C-xn": history-search-forward
"\ep": history-search-backward
"\en": history-search-forward
"\eOM": accept-line
"\eO@": abort
"\eO\000": abort
"\eOo": abort
"\eOk": abort
"\eOm": abort
"\eOj": abort
"\e[2~": abort
"\e[3~": abort
"\e[5~": abort
"\e[6~": abort
set convert-meta off
set output-meta on
set input-meta on

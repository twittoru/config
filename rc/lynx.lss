# lynx.lss for euske

# Special styles - not corresponding directly to HTML tags
#	alert	- status bar, when message begins "Alert".
#	alink	- active link
#	normal	- default attributes
#	status	- status bar
#	whereis	- whereis search target
#
alink:		reverse:	black:yellow
status:		reverse:	black:green
alert:		bold:		yellow:red
whereis:	reverse:	brown:red

title:		normal:		default
hr:		normal:		default
h1:		normal:		yellow:blue
h2:		normal:		white:blue
h3:		normal:		brightcyan
h4:		normal:		brightgreen
h5:		normal:		yellow
h6:		normal:		white

a:		underline:	cyan

strong:		normal:		red
b:		normal:		red
big:		normal:		red
em:		normal:		green
i:		normal:		green
small:		normal:		green

img:		normal:		magenta
li:		normal:		green
pre:		normal:		magenta
blockquote:	normal:		green
caption:	normal:		green
address:	normal:		white
label:		normal:		white
q:		normal:		magenta
tt:		normal:		magenta
code:		normal:		magenta
sup:		normal:		blue
sub:		normal:		blue
